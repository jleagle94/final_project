<?xml version="1.0"?>
<run>
    <tool>
        <title>Integrating and Graphing Calculator</title>
        <about>Press Simulate to view results.</about>
        <command>python @tool/king_model.py @driver</command>
    </tool>
    <input>
        <string id="king(x,y)">
            <about>
                <label>king</label>
                <hints></hints>
            </about>
            <size>30x5</size>
	    <default></default>
        </string>
        <number id="x">
            <about>
		<label>From x</label>
		<description>This is the starting point.</description>
	    </about>
            <default>0</default>
        </number>
        <number id="x">
	    <about>
	  	<label>To x</label>
		<description>This is the ending point</description>
	    </about>
            <default>100</default>
        </number>
        <number id="y_init">
            <about>
	        <label>initial y</label>
		<description>The value of a at the minimum X</description>
	    </about>
            <default>0</default>
        </number>
	
    </input>
    </run>
